    "@types/express": "^4.17.2",
    "@types/jest": "25.1.1",
    "@types/lodash": "^4.14.158",
    "@types/minio": "^7.0.5",
    "@types/node": "^13.1.6",
    "@types/supertest": "^2.0.8",
    "@types/zarinpal-checkout": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
